	<queryString>
		<![CDATA[select s.batchnumber, s.manufacturer,  s.datereceived, s.expirydate, s.unitsreceived, s.id, ceil(sum(pt.amount::real/d.packsize::real)) as used 
from clinic as c, drug as d, stock as s

full join (
select packdrug.amount, packdrug.stock, packdrug.parentpackage
from packageddrugs as packdrug, package as pack
where packdrug.parentpackage=pack.id 
and pack.packdate between $P{startDate} and $P{endDate}
) 

as pt on pt.stock = s.id 
where s.drug = d.id 
and s.clinic = c.id
and c.id = $P{clinic} 
and d.id = $P{drugId}
and s.datereceived between $P{startDate} and $P{endDate}

group by s.batchnumber, s.datereceived, s.expirydate, s.unitsreceived, s.id, d.packsize, s.manufacturer

order by s.datereceived asc]]>
	</queryString>
