	<queryString>
		<![CDATA[select distinct pd.stock, pd.amount as amountIssued, pack.packageid, pack.packdate as packdate, pack.prescription, pat.patientid, sum(pd.amount) as totalDispensed

from packageddrugs as pd, package as pack, prescription as pre, patient as pat

where pd.stock = $P{stockId}
and pd.parentpackage = pack.id
and pack.prescription = pre.id 
and pre.patient = pat.id

group by pd.stock, pd.amount, pack.packageid, pack.packdate, pack.prescription, pat.patientid]]>
	</queryString>
