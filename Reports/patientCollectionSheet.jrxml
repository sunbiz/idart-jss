	<queryString>
		<![CDATA[SELECT
     patient."idnum" AS patient_idnum,
     patient."lastname" AS patient_lastname,
     patient."firstnames" AS patient_firstnames,
     patient."homephone" AS patient_homephone,
     patient."dateofbirth" AS patient_dateofbirth,
     patient."sex" AS patient_sex,
     patient."address1" AS patient_address1,
     patient."address2" AS patient_address2,
     patient."address3" AS patient_address3,
     patient."patientid" AS patient_patientid,
     patient."workphone" AS patient_workphone,
     patient."id" AS patient_id,
     patient."patientid" AS patient_patientid,
     patient."nextofkinname" As patient_treatmentsuppname,
     patient."nextofkinphone" AS patient_treatmentsuppnumber,
     patientattribute."value" AS arv_start_date,
     patient."cellphone" AS patient_cellphone,
     pregnancy."confirmdate" AS pregn_confirmdate,
     latestpackage."packdate" as packdate,
     latestpackage."packageid" as packid
FROM
    "patient" patient 
	LEFT OUTER JOIN "patientattribute" patientattribute
		ON "patient"."id" = patientattribute."patient"
	LEFT OUTER JOIN "attributetype" attributetype 
		ON (patientattribute.type_id = attributetype.id AND attributetype.name = 'ARV Start Date')
	LEFT OUTER JOIN "pregnancy" pregnancy
		ON (pregnancy.patient = patient.id AND pregnancy.enddate is null),
(select package."packdate"as packdate , package."packageid" as packageid
	from package, 
	(select id, date as prescrdate, prescriptionid from prescription where patient = 
               (select id from patient where $P{patientid} = patient.patientid)
		order by date desc limit 1) as presc
	where package."prescription" = presc."id" 
	order by packdate desc limit 1
) as latestpackage
where patient.patientid = $P{patientid}]]>
	</queryString>
