	<queryString>
		<![CDATA[select date_part('month', $P{startDate}::timestamp)::integer as monthindex, 
a.patcount as adultPatientsStarting, 
b.patcount as adultPatientsTotal,
c.patcount as paedsPatientsTotal, 
d.patcount as paedsPatientsStarting,
e.patcount as adultsEverInit,
f.patcount as paedsEverInit,
g.epcount as adultsStartedEp,
h.epcount as paedsStartedEp,
i.patcount as adultsSeen,
j.patcount as paedsSeen,
i.distinctpatcount as uniqueAdultsSeen,
j.distinctpatcount as uniquePaedsSeen,
k.patcount as  adultsDied,
l.patcount as  paedsDied,
m.epcount  as adultsInactive,
n.epcount  as paedsInactive,
u.patcount as adultsDefaulting,
v.patcount as paedsDefaulting,
w.patcount as adultsDefaultingPreART,
x.patcount as paedsDefaultingPreART,
y.patcount as adultPatientsStartingNonART, 
z.patcount as paedsPatientsStartingNonART

from


(select count(a1.patient) as patcount from (select prescription.patient, min(pickupdate) 
from package,prescription, patient, clinic, episode ep
where prescription.id = package.prescription
and clinic.id = ep.clinic
and ep.patient = patient.id
and ((ep.startDate::date between $P{startDate}::date and $P{endDate}::date) or
(ep.stopDate::date between $P{startDate}::date and $P{endDate}::date) or
(ep.startDate::date < $P{startDate}::date and ep.stopDate is null))
and ep.startreason like 'New Patient'
and clinic.clinicName like $P{clinic}
and patient.dateofbirth < $P{cutoffDate}
and patient.id = prescription.patient
and package.drugTypes like 'ARV'
group by prescription.patient
having min(pickupdate) between $P{startDate}::timestamp and ($P{endDate}::timestamp)) as a1) as a,


(select count(b1.patient) as patcount from 
(select distinct patient.id as patient
from prescription,patient,clinic, episode ep
where patient.id=prescription.patient
and clinic.id = ep.clinic
and ep.patient = patient.id
and ((ep.startDate::date between $P{startDate}::date and $P{endDate}::date) or
(ep.stopDate::date between $P{startDate}::date and $P{endDate}::date) or
(ep.startDate::date < $P{startDate}::date and ep.stopDate is null))
and clinic.clinicName like $P{clinic}
and patient.dateofbirth < $P{cutoffDate}
and prescription.drugtypes like 'ARV'
and ((prescription.enddate is null and prescription.date < $P{endDate}) 
or ($P{endDate} between prescription.date and prescription.enddate))) as b1) as b,

(select count(c1.patient) as patcount from 
(select distinct prescription.patient 
from prescription,patient,clinic, episode ep
where patient.id=prescription.patient
and clinic.id = ep.clinic
and ep.patient = patient.id
and ((ep.startDate::date between $P{startDate}::date and $P{endDate}::date) or
(ep.stopDate::date between $P{startDate}::date and $P{endDate}::date) or
(ep.startDate::date < $P{startDate}::date and ep.stopDate is null))
and clinic.clinicName like $P{clinic}
and patient.dateofbirth >= $P{cutoffDate}
and prescription.drugtypes like 'ARV'
and ((prescription.enddate is null and prescription.date < $P{endDate}) 
or ($P{endDate} between prescription.date and prescription.enddate)))as c1) as c,

(select count(d1.patient) as patcount from (select prescription.patient, min(pickupdate) 
