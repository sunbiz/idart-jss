group by pat.id)as k1) as k,

(select count(l1.patID) as patcount from (select pat.id as patID
from patient as pat, prescription as pre, clinic, episode ep, prescribeddrugs as pd, drug as d
where pat.id = pre.patient
and clinic.id = ep.clinic
and ep.patient = pat.id
and ep.stopReason like 'Deceased'
and ep.stopdate <= ($P{endDate}::date)
and pre.endDate = ep.stopDate
and pd.prescription = pre.id
and pd.drug = d.id
and d.sidetreatment like 'F'
and clinic.clinicName like $P{clinic}
and pat.dateofbirth >= $P{cutoffDate}
group by pat.id)as l1) as l,

(select count(m1.epID) as epcount from (select ep.id as epID
from patient, clinic, episode ep
where clinic.id = ep.clinic
and ep.patient = patient.id
and ep.stopDate::date between $P{startDate}::date and $P{endDate}::date
and clinic.clinicName like $P{clinic}
and patient.dateofbirth < $P{cutoffDate}) as m1) as m,

(select count(n1.epID) as epcount from (select ep.id as epID
from patient, clinic, episode ep
where clinic.id = ep.clinic
and ep.patient = patient.id
and ep.stopDate::date between $P{startDate}::date and $P{endDate}::date
