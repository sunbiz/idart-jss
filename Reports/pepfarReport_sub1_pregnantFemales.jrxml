	<queryString>
		<![CDATA[select e.patcount as atStart,
f.patcount as atEnd,
g.patcount as newInMonth,
h.patcount as transferredInMonth,
i.patcount as currentlyOnART

from

(select count (e1.pat) as patcount from 
(select distinct prescription.patient as pat
from prescription, package, patient,clinic, pregnancy as preg
where package.prescription = prescription.id
and prescription.patient = patient.id
and clinic.id = package.clinic
and package.pickupdate::date < ($P{startDate}::date)
and package.drugTypes like 'ARV' 
and clinic.clinicName like $P{clinic}
and preg.patient = patient.id
and (((preg.enddate is null) and (preg.confirmdate::date < $P{endDate}::date)) or  ($P{endDate}::date between preg.confirmdate and preg.enddate))
)
as e1) as e,

(select count (f1.pat) as patcount from 
(select distinct prescription.patient as pat
from prescription, package, patient,clinic, pregnancy as preg, episode ep
where package.prescription = prescription.id
and prescription.patient = patient.id
and clinic.id = ep.clinic
and ep.patient = patient.id
and package.pickupdate::date <= ($P{endDate}::date)
and clinic.clinicName like $P{clinic}
and preg.patient = patient.id
and (((preg.enddate::date is null) and (preg.confirmdate < $P{endDate}::date)) or  ($P{endDate}::date between preg.confirmdate::date and preg.enddate::date))
)
as f1) as f,

(select count(g1.pat) as patcount from (select prescription.patient as pat, min(pickupdate) 
from package,prescription, patient, clinic, pregnancy as preg, episode ep
where prescription.id = package.prescription
and clinic.id = ep.clinic
and clinic.clinicName like $P{clinic}
and preg.patient = patient.id
and (((preg.enddate is null) and (preg.confirmdate::date < $P{endDate}::date)) or  ($P{endDate}::date between preg.confirmdate::date and preg.enddate::date))
and patient.id = prescription.patient
and ep.patient = patient.id
and ((ep.startDate::date between $P{startDate}::date and $P{endDate}::date) or
(ep.stopDate::date between $P{startDate}::date and $P{endDate}::date) or
(ep.startDate::date < $P{startDate}::date and ep.stopDate is null))
and ep.startreason ilike 'New Patient'
and package.drugTypes like 'ARV' 
group by prescription.patient
having min(pickupdate::date) between $P{startDate}::date and $P{endDate}::date) as g1) as g,

(select count(h1.pat) as patcount from (select prescription.patient as pat, min(pickupdate) 
from package,prescription, patient, clinic, pregnancy as preg, episode ep
where prescription.id = package.prescription
and clinic.id = ep.clinic
and clinic.clinicName like $P{clinic}
and preg.patient = patient.id
and (((preg.enddate is null) and (preg.confirmdate < $P{endDate}::date)) or  ($P{endDate}::date between preg.confirmdate::date and preg.enddate::date))
and patient.id = prescription.patient
and ep.patient = patient.id
and ((ep.startDate::date between $P{startDate}::date and $P{endDate}::date) or
(ep.stopDate::date between $P{startDate}::date and $P{endDate}::date) or
(ep.startDate::date < $P{startDate}::date and ep.stopDate is null))
and ep.startReason ilike 'Transfer%'
and package.drugTypes like 'ARV' 
group by prescription.patient
having min(pickupdate::date) between $P{startDate}::date and $P{endDate}::date) as h1) as h,

(select count(i1.pat) as patcount from (select distinct patient.id as pat
from prescription,patient,clinic, pregnancy as preg, episode ep
where patient.id=prescription.patient
and clinic.id = ep.clinic
and ep.patient = patient.id
and ((ep.startDate::date between $P{startDate}::date and $P{endDate}::date) or
(ep.stopDate::date between $P{startDate}::date and $P{endDate}::date) or
(ep.startDate::date < $P{startDate}::date::date and ep.stopDate::date is null))
and clinic.clinicName like $P{clinic}
and preg.patient = patient.id
and (((preg.enddate is null) and (preg.confirmdate::date < $P{endDate}::date)) or  ($P{endDate}::date between preg.confirmdate::date and preg.enddate::date))
and prescription.drugtypes like 'ARV'
and ((prescription.enddate is null and prescription.date < $P{endDate}) 
or ($P{endDate} between prescription.date and prescription.enddate))) as i1) as i]]>
	</queryString>
